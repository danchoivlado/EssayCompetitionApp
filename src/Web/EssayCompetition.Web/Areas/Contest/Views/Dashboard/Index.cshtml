@{
    this.ViewData["Title"] = "Dashboard Page";
    this.ViewData["ImagePath"] = "img/home-bg.jpg";
    this.Layout = "_CustomLayout";
    this.ViewData["ImageH1Text"] = "Contest";
    this.ViewData["ImageSpanText"] = "Contest has begun write your essay";
}
<h1>@this.ViewData["Title"]</h1>

<div class="container">
    <div class="col-lg-12 col-md-12 mx-auto">
        @if (TempData["FormResult"] != null)
        {
            <div class="alert alert-info fade show" role="alert">
                @TempData["FormResult"]
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        }
        <div class="d-flex justify-content-between">
            <div>
                <a asp-area="Contest" asp-controller="Create"><i class="fa fa-book-reader">Create essay</i></a>
            </div>
        </div>
    </div>
</div>
<hr>