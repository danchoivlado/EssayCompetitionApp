@using EssayCompetition.Web.ViewModels.Calendars
@model CalendarViewModel
@{
    this.ViewData["Title"] = "Home Page";
    this.ViewData["ImagePath"] = "img/about-bg.jpg";
    this.Layout = "_CustomLayout";
}
<div class="container">
    <div class="row">
        <div class="col-lg-12 col-md-12 mx-auto">
            <calendar month="@Model.Month" year="@Model.Year" events="@Model.Events" />
        </div>
    </div>
</div>

<hr>
